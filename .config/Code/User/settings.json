{
	"editor.tabSize": 3,
	"editor.renderWhitespace": "all",
	"editor.insertSpaces": false,
	"git.autorefresh": false,
	"files.insertFinalNewline": true,
	"files.trimTrailingWhitespace": true,
	"workbench.iconTheme": "material-icon-theme",
	"workbench.startupEditor": "newUntitledFile",
	"git.enableSmartCommit": true,
	"window.zoomLevel": -1,
	"material-icon-theme.activeIconPack": "react_redux",
	"material-icon-theme.folders.theme": "classic",
	"importSorter.importStringConfiguration.tabType": "tab",
	"importSorter.importStringConfiguration.tabSize": 3,
	"importSorter.sortConfiguration.importPaths.order": "lowercaseLast",
	"importSorter.sortConfiguration.importMembers.order": "lowercaseLast",
	"files.exclude": {
		"**/__generated__/": true,
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/node_modules": true,
		"**/dist": true,
		"**/vendor": true,
		"**/*.lock": true,
		"**/*.scss.d.ts": true,
		"**/schema.json": true,
		"**/*.log": true
	},
	"java.home": "/usr/lib/jvm/java-8-openjdk",
	"explorer.confirmDragAndDrop": false
}
