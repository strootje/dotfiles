{
	"editor.tabSize": 3,
	"editor.renderWhitespace": "all",
	"editor.insertSpaces": false,
	"git.autorefresh": false,
	"files.insertFinalNewline": true,
	"files.trimTrailingWhitespace": true,
	"workbench.iconTheme": "material-icon-theme",
	"workbench.startupEditor": "newUntitledFile",
	"git.enableSmartCommit": true,
	"window.zoomLevel": -1,
	"material-icon-theme.activeIconPack": "react_redux",
	"material-icon-theme.folders.theme": "classic",
	"importSorter.importStringConfiguration.tabType": "tab",
	"importSorter.importStringConfiguration.tabSize": 3,
	"importSorter.sortConfiguration.importPaths.order": "lowercaseLast",
	"importSorter.sortConfiguration.importMembers.order": "lowercaseLast",
	"files.exclude": {
		"**/__generated__/": true,
		"**/.DS_Store": true,
		"**/.git": true,
		"**/.hg": true,
		"**/.nyc_output": true,
		"**/.svn": true,
		"**/*.lock": true,
		"**/*.log": true,
		"**/*.scss.d.ts": true,
		"**/CVS": true,
		"**/dist": true,
		"**/node_modules": true,
		"**/schema.json": true,
		"**/vendor": true,
		"**/.classpath": true,
		"**/.project": true,
		"**/.settings": true,
		"**/.factorypath": true
	},
	"explorer.confirmDragAndDrop": false,
	"typescript.updateImportsOnFileMove.enabled": "always",
	"gitlens.views.repositories.location": "scm",
	"gitlens.views.fileHistory.location": "scm",
	"gitlens.views.lineHistory.location": "scm",
	"gitlens.views.compare.location": "scm",
	"gitlens.views.search.location": "scm",
	"mochaExplorer.files": [ "test/**/*.ts", "test/**/*.tsx" ],
	"mochaExplorer.env": {
		"NODE_ENV": "test"
	},
	"editor.codeLens": false,
	"explorer.confirmDelete": false,
	"editor.suggestSelection": "first",
	"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
	"dart.checkForSdkUpdates": false,
	"dart.closingLabels": false,
	"dart.enableSdkFormatter": false,
	"[dart]": {
		"editor.tabSize": 3,
		"editor.insertSpaces": false,
		"editor.detectIndentation": false
	},
	"breadcrumbs.enabled": true
}
